package objetos;

import java.io.IOException;

import javax.imageio.ImageIO;


import entidade.Entidade;
import main.GamePainel;

public class Moeda extends Entidade
{

	public Moeda(GamePainel gp) throws IOException
	{
		super(gp);
		
		name = "moeda";
		parado = ImageIO.read(getClass().getResourceAsStream("/itens/moeda.png"));
		
		colision = true;
		
	
	}
	
}
