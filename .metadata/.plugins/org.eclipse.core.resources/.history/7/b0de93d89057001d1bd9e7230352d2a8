package main;

import entidade.Enpc;
import objetos.Moeda;

public class setAssets 
{

	GamePainel gp;
	
	public setAssets(GamePainel gp)
	{
		this.gp = gp;
	}
	
	
	
	public void setNpc()
	{
		gp.npc[0] = new Enpc(gp);
		gp.npc[0].worldX = 968;
		gp.npc[0].worldY = 900;
	}	
	
	public void Itens()
	{
		gp.obj[0] = new Moeda();
		gp.obj[0].worldX = gp.tileSize*26;
		gp.obj[0].worldY = gp.tileSize*7; 
	}
	
	
}
