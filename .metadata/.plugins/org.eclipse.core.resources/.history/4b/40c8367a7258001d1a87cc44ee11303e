package objetos;

import java.io.IOException;

import javax.imageio.ImageIO;

import entidade.Entidade;
import main.GamePainel;

public class Livro extends Entidade
{

	public Livro(GamePainel gp) throws IOException
	{
		super(gp);
		
		name = "livro";
		
		Image = ImageIO.read(getClass().getResourceAsStream("/itens/livro.png"));
		
	}
	
}
