package objetos;

import javax.imageio.ImageIO;

import entidade.Attack;
import main.GamePainel;

public class Projetil extends Attack
{

	GamePainel gp;
	
	public Projetil(GamePainel gp) 
	{
		super(gp);
		
		this.gp = gp;
		name = "Projetil";
		speed = 3;
		
		
		getImage();
	}
	
	
	public void getImage()
	{
		try
		{
			projetil = ImageIO.read(getClass().getResourceAsStream("/Batalha/projetil.png"));
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
	}

}
